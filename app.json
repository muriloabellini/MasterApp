{
  "expo": {
    "name": "MasterPagamentos",
    "slug": "bolt-expo-nativewind",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icone.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-web-browser",
      [
        "expo-notifications",
        {
          "icon": "./assets/images/icone.png",
          "color": "#1C1B1F",
          "mode": "production"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "android": {
      "package": "com.MasterPagamentos.Bellinitech"
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "362e30bd-7e5e-44fe-a727-83d8cc8a919d"
      }
    }
  }
}
